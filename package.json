{
  "name": "bootpack",
  "version": "0.10.1",
  "author": "ZachTRice",
  "description": "A webpack and bootstrap boilerplate template for building web pages quickly.",
  "license": "MIT",
  "homepage": "https://github.com/bootpack/bootpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootpack/bootpack.git"
  },
  "bugs": {
    "url": "https://github.com/bootpack/bootpack/issues"
  },
  "keywords": [
    "webpack",
    "template",
    "webpage",
    "starter",
    "website"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "test": "run-s lint:*",
    "lint": "run-s lint:*",
    "lint:css": "stylelint ./src/css/*.css",
    "lint:js": "eslint .",
    "compress:images": "node ./tools/compress-images.js",
    "generate:images": "node ./tools/generate-images.js"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.2",
    "compress-images": "^1.5.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-promise": "^2.0.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "exports-loader": "^0.7.0",
    "favicons": "^5.3.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "js-image-generator": "^1.0.2",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.0.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
